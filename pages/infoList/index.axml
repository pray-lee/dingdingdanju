<view class="info-wrapper">
    <view class="search-wrapper">
        <input type="text" placeholder="搜索" onInput="onInput">
    </view>
    <scroll-view scroll-y class="scroll-info-list">
        <view class="info-item" a:for="{{searchResult}}" data-id="{{item.id}}" onTap="goBack">
            {{item.name}}
        </view>
    </scroll-view>
</view>
