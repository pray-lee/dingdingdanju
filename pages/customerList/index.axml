<view class="info-wrapper">
    <view class="search-wrapper">
        <input type="text" placeholder="搜索" onInput="onInput">
    </view>
    <scroll-view scroll-y class="scroll-info-list {{isPhoneXSeries ? 'iphoneX' : ''}}">
        <view class="info-item" a:for="{{searchResult}}" data-customerId="{{item.customerId}}" data-name="{{item.customer.customerName}}" onTap="goBack">
            {{item.customer.customerName}}
        </view>
    </scroll-view>
</view>
